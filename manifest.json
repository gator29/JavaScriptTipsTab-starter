{
  "name": "JavaScript Tips Tab",
  "manifest_version": 2,
  "description": "Codecademy JavaScript tips when you open a new tab in Chrome",
  "version": "0.0.0.1",
  "browser_action": {
        "default_title": "Javascipt Tips Tab"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": ["index.html"],
  "icons": {
    "128": "icon.png"
  }
}
